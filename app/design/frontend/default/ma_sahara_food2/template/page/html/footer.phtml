<script>
jQuery(document).ready(function() {
var stickyNavTop = jQuery('.box-header').offset().top;
 
var stickyNav = function(){
var scrollTop = jQuery(window).scrollTop();
      
if (scrollTop > stickyNavTop) { 
    jQuery('.box-header').addClass('sticky');
    jQuery('.main-container').css({'margin-top': '165px'});
	
} else {
    jQuery('.box-header').removeClass('sticky'); 
    jQuery('.main-container').removeAttr('style');
}
};
 
stickyNav();
 
jQuery(window).scroll(function() {
    stickyNav();
});
});
</script>

